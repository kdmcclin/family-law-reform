<div class="row">
    <div class="large-12 columns">
    	<h2 class="stateheader">South Carolina</h2>
    	<%= form_tag(south_carolina_path, :method => "get", id: "search-form") do %>
	  		<%= text_field_tag :search, params[:search], placeholder: "Search for a Person Within This State" %>
  			<%= submit_tag "Search", :class => "button radius" %>
		<% end %>
        <%= render partial: 'report_display' %>
    </div>
</div>